<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LocusImage</servlet-name>
        <servlet-class>com.locus305.servlets.LocusImage</servlet-class>
        <init-param>
            <param-name>width</param-name>
            <param-value>200</param-value>
        </init-param>
        <init-param>
            <param-name>height</param-name>
            <param-value>200</param-value>
        </init-param>
        <init-param>
            <param-name>points</param-name>
            <param-value>50</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>TestEmail</servlet-name>
        <servlet-class>com.locus305.servlets.TestEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestName</servlet-name>
        <servlet-class>com.locus305.servlets.TestName</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterUser</servlet-name>
        <servlet-class>com.locus305.servlets.RegisterUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LocusLogo</servlet-name>
        <servlet-class>com.locus305.servlets.LocusLogo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.locus305.servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.locus305.servlets.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUserInfo</servlet-name>
        <servlet-class>com.locus305.servlets.EditUserInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LikePost</servlet-name>
        <servlet-class>com.locus305.servlets.LikePost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnlikePost</servlet-name>
        <servlet-class>com.locus305.servlets.UnlikePost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LikeComment</servlet-name>
        <servlet-class>com.locus305.servlets.LikeComment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnlikeComment</servlet-name>
        <servlet-class>com.locus305.servlets.UnlikeComment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendMessage</servlet-name>
        <servlet-class>com.locus305.servlets.SendMessage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPost</servlet-name>
        <servlet-class>com.locus305.servlets.AddPost</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestEmail</servlet-name>
        <url-pattern>/testEmail.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestName</servlet-name>
        <url-pattern>/testName.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterUser</servlet-name>
        <url-pattern>/RegisterUser.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LocusImage</servlet-name>
        <url-pattern>/LocusImage2.png</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LocusLogo</servlet-name>
        <url-pattern>/LocusImage.png</url-pattern>
        <url-pattern>/LocusLogo.png</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUserInfo</servlet-name>
        <url-pattern>/EditUserInfo.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LikePost</servlet-name>
        <url-pattern>/LikePost.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnlikePost</servlet-name>
        <url-pattern>/UnlikePost.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LikeComment</servlet-name>
        <url-pattern>/LikeComment.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnlikeComment</servlet-name>
        <url-pattern>/UnlikeComment.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendMessage</servlet-name>
        <url-pattern>/SendMessage.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPost</servlet-name>
        <url-pattern>/AddPost.htm</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
